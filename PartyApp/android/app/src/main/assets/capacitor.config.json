{
	"appId": "com.charke.partyapp",
	"appName": "Motive",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "build",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": false,
			"backgroundColor": "#00000000",
			"androidSplashResourceName": "applogo",
			"spinnerColor": "#FFFF00",
			"showSpinner": false,
			"splashFullScreen": true,
			"splashImmersive": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"cordova": {}
}
